<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".MainActivity"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="-325dp">


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView001"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="#7cfc00" />

            <ImageView
                android:id="@+id/imageView002"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="#87cefa" />

            <ImageView
                android:id="@+id/imageView003"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="#7cfc00" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView004"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="#87cefa" />

            <ImageView
                android:id="@+id/imageView005"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="#7cfc00" />

            <ImageView
                android:id="@+id/imageView006"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="#87cefa" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView007"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="#7cfc00" />

            <ImageView
                android:id="@+id/imageView008"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="#87cefa" />

            <ImageView
                android:id="@+id/imageView009"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:background="#7cfc00" />

        </TableRow>
    </TableLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="95dp"
        android:layout_height="62dp"
        android:text="スタンプ減らす"
        android:layout_marginTop="400dp"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout" />

</androidx.constraintlayout.widget.ConstraintLayout>